    "husky": "^7.0.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "redux-logger": "^3.0.6",
    "stubby": "^5.1.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/store.ts"
    ]
  }
