  "scripts": {
    "start": "SERVICE_URL=https://www.carboninterface.com react-scripts start",
    "start:stubs": "SERVICE_URL=http://localhost:8882 concurrently --kill-others \"react-scripts start\" \"yarn stubby\"",
    "stubby": "stubby -d stubs/stubby-config.yml -w -q",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:watch": "react-scripts test --coverage -w",
    "eject": "react-scripts eject",
    "prettier": "prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/css-mediaquery": "^0.1.1",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "concurrently": "^6.3.0",
    "css-mediaquery": "^0.1.2",
    "eslint": "7.32.0",
